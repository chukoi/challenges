    "prettier:fix": "npx prettier --write \"**/*.{ts,js}\""
  },
  "dependencies": {
    "@types/jest": "^29.5.12",
    "@types/react": "^18.2.78",
    "chai": "^5.1.0",
    "jest": "^29.7.0",
    "npx": "^10.2.2",
    "prettier": "^3.2.5",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-scripts": "^5.0.1",
    "typescript": "^5.4.4"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
