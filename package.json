  "scripts": {
    "start": "react-scripts start",
    "reset": "rm -rf node_modules && yarn && yarn build",
    "build": "NODE_OPTIONS=\"--max-old-space-size=4096\" rm -rf compiled && tsc --version && tsc -b",
    "start:timeInWords": "node ./compiled/timeInWords.js",
